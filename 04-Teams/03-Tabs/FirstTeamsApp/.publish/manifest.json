{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.8/MicrosoftTeams.schema.json",
    "manifestVersion": "1.8",
    "version": "{version}",
    "id": "609993e0-0739-11eb-9767-47ddb7738e37",
    "packageName": "firstteamsapp",
    "developer": {
        "name": "Integrations",
        "websiteUrl": "{baseUrl0}",
        "privacyUrl": "{baseUrl0}/privacy.html",
        "termsOfUseUrl": "{baseUrl0}/tou.html"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "{appname}",
        "full": "{fullappname}"
    },
    "description": {
        "short": "TODO: add short description here",
        "full": "TODO: add full description here"
    },
    "accentColor": "#D85028",
    "staticTabs": [
        {
            "entityId": "5c63200b-0843-4160-aa06-f9a707cd9773",
            "name": "First Demo",
            "contentUrl": "{baseUrl0}/firstDemoTab/?name={loginHint}&tenant={tid}&theme={theme}",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "personalTab",
            "name": "Personal Tab ",
            "contentUrl": "{baseUrl0}/firstDemoTab/personal.html",
            "websiteUrl": "{baseUrl0}",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}